{
  "env": {
      "browser": true,
      "es6": true
  },
  "extends": "airbnb",
  "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
  },
  "parser": "babel-eslint",
  "parserOptions": {
      "ecmaFeatures": {
          "jsx": true
      },
      "ecmaVersion": 2018,
      "sourceType": "module"
  },
  "plugins": [
      "react",
      "jsx-ally",
      "import"
  ],
  "ecmaFeatures": {
      "arrowFunctions": true
  },
  "rules": {
      "no-use-before-define": [0],
      "react/jsx-filename-extension": [0],
      "jsx-ally/anchor-is-valid": [0],
      "no-confusing-arrow": [0],
      "no-underscore-dangle": [0],
      "import/no-extraneous-depencies": [0]
  }
}